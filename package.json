{
  "name": "crlf-es",
  "version": "1.1.0",
  "description": "Convert line endings",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "bin/crlf-es.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
